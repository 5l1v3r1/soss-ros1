<?xml version="1.0"?>
<package format="2">
  <name>soss-ros1</name>
  <version>2.0.0</version>
  <description>
    A soss plugin that translates from soss to ROS1
  </description>

  <maintainer email="grey@openrobotics.org">Grey</maintainer>
  <license>Apache License 2.0</license>

  <depend>roscpp</depend>

  <build_depend>catkin</build_depend>
  <build_depend>soss-genmsg</build_depend>

  <exec_depend>cmake</exec_depend>
  <export>
    <build_type>cmake</build_type>
  </export>

</package>
